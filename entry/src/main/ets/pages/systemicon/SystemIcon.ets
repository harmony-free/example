  @Builder
  itemBuilder(data:object | Resource,index:number){
    Image(data as Resource).aspectRatio(1).onClick(()=>{
      console.log(this.dataName[index]);
      AlertDialog.show({message:this.dataName[index]})
    })
  }
  @Builder
  footerBuilder(){

  }

  /**
   * 刷新
   */
  upData = (page?: PageFace):Promise<boolean | Array<object>> => {
    return new Promise<boolean | Array<object>>((res, rej) => {
      let list:RefreshModel[] = []
      this.dataSource.forEach((e)=>{
        list.push(new RefreshModel(e))
      })
      res(list)
    })
  }

  build() {
    Column(){
      NavBar({title:"系统图标"})
      RefreshGrid({
        itemBuilderParam:this.itemBuilder.bind(this),
        refresh:new RefreshUtils({data:this.dataSource}),
        footerBuilderParam:this.footerBuilder
      }).height(global.getContentTabBar())
    }
  }
